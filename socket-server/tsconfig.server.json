{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2017",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "strict": true,
    "noEmit": false,
    "isolatedModules": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["socket-server/**/*.ts"],
  "exclude": ["node_modules"]
}